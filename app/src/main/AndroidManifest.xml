<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asadfiaz.BasicAlgorithmVisualizer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.MainActivity" />
        <activity
            android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.ExpandedChildActivity"
            android:label="@string/title_activity_expanded_child"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.SignUp" />
        <activity android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.Teacher" />
        <activity android:name="com.example.asadfiaz.BasicAlgorithmVisualizer.QuizActivity"></activity>
    </application>

</manifest>